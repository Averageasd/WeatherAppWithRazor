@model List<WeatherData>
<!DOCTYPE html>
<html lang="en">
<head>
	<title>All weather data</title>
	<meta charset="utf-8" />
	<link href="~/StyleSheet.css" rel="stylesheet" />
</head>
<body>
	<h1>All Weather data</h1>
	<div>
		<ul>
			@foreach (WeatherData weatherData in Model)
			{
				<li>
					<h3>Code: @weatherData.CityUniqueCode</h3>
					<p>City: @weatherData.CityName</p>
					<p>Date: @weatherData.DateAndTime</p>
					<p>Temperature: @weatherData.TemperatureFahrenheit farenheit</p>
					<div>
						<a href="/weather/@weatherData.CityUniqueCode">detail</a>
					</div>
				</li>
			}
		</ul>
	</div>
</body>
</html>